    "\n",
    "# Define generalized coordinate and its time derivative\n",
    "q = sp.Function('q')(t)\n",
    "q_dot = q.diff(t)\n",
    "\n",
    "# Define any required system parameters (e.g., mass)\n",
    "m = sp.Symbol('m', positive=True)"
